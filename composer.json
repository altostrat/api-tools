{
    "name": "altostrat/tools",
    "description": "Tooling for Laravel microservices",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Altostrat\\Tools\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hannes Kruger",
            "email": "support@altostrat.io"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Altostrat\\Tools\\AltostratServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^9.21|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.21|^10.0|^11.0|^12.0",
        "illuminate/log": "^9.21|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.21|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.21|^10.0|^11.0|^12.0",
        "auth0/auth0-php": "^8.7",
        "nyholm/psr7": "^1.8",
        "aws/aws-sdk-php": "^3.269"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
