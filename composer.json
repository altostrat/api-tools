{
    "name": "mikrocloud/mikrocloud",
    "description": "Tooling for MikroCloud microservices",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Mikrocloud\\Mikrocloud\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hannes Kruger",
            "email": "hannes.kruger@mikrocloud.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Mikrocloud\\Mikrocloud\\MikrocloudServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/http": "^9.21|^10.0",
        "illuminate/log": "^9.21|^10.0",
        "illuminate/routing": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0",
        "auth0/auth0-php": "^8.7",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
